@Article{Martin2009,
  Title                    = {LSO-Based Single Crystal Film Scintillator for Synchrotron-Based Hard X-Ray Micro-Imaging},
  Author                   = {Martin, T. and Douissard, P.-A. and Couchaud, M. and Cecilia, A. and Baumbach, Tilo and Dupre, K. and Rack, A.},
  Journal                  = {Nuclear Science, IEEE Transactions on},
  Year                     = {2009},

  Month                    = {June},
  Number                   = {3},
  Pages                    = {1412-1418},
  Volume                   = {56},

  Abstract                 = {X-ray detector systems are powerful tools: in combination with tomographic methods they provide volumetric data of samples in a non-destructive manner which is of high interest for, e.g., biology, medicine or materials research. The detector able to provide images with submicrometer spatial resolution frequently consists of a scintillator screen, light microscopy optics and a digital camera. Here, the scintillator converts the X-rays into a visible light image which is projected onto the camera by the light optics. In order to perform high resolution imaging Single Crystal Film (SCF) scintillators 1 mum to 30 mum thin are required due to the limited depth of focus of the microscopy optics. Thin SCFs can be obtained via liquid phase epitaxy (LPE). A drawback is that a detector working with SCFs suffers from low efficiency (2\% at 50 keV) owing to their limited thickness. The detective quantum efficiency (DQE) is here mainly limited by the low absorption of X-rays and the light yield in the thin scintillator layer. Performances, i.e absorption, light yield, afterglow of operational systems at the European Synchrotron Radiation Facility (ESRF) using YAG:Ce (Y3Al5O12:Ce), LAG:Eu (Lu3Al5O12:Eu) and GGG:Eu (Gd3Ga5O12:Eu) scintillators will be presented and compared to new LSO:Tb (Lu2SiO5:Tb) scintillators developed in the framework of an European project , . A new concept to improve the efficiency of detection in the 20 keV - 40 keV energy range with 1 mum spatial resolution will be presented. This concept based on multilayer scintillators is realised by the LPE process as well. First results will be illustrated with X-ray images and will demonstrate the absorption efficiency improvement of the X-ray detector. The expected performance is 7 times better than the LAG-based scintillators.},
  Doi                      = {10.1109/TNS.2009.2015878},
  File                     = {:Files/Martin-2009-LSOBasedSingleCrystalFilmScintillator.pdf:PDF},
  ISSN                     = {0018-9499},
  Keywords                 = {X-ray absorption;X-ray apparatus;X-ray detection;X-ray microscopy;cameras;position sensitive particle detectors;solid scintillation detectors;thin film devices;GGG:Eu scintillators;Gd3Ga5O12:Eu;LAG:Eu scintillators;LSO:Tb scintillators;Lu2SiO5:Tb;Lu3Al5O12:Eu;X-ray absorption;X-ray detector systems;Y3Al5O12:Ce;YAG:Ce scintillators;afterglow;detective quantum efficiency;digital camera;electron volt energy 20 keV to 40 keV;electron volt energy 50 keV;light microscopy optics;light yield;liquid phase epitaxy;scintillator screen;single crystal film scintillator;size 1 mum to 30 mum;synchrotron-based hard X-ray microimaging;tomographic methods;visible light image;Biomedical imaging;Biomedical optical imaging;Electromagnetic wave absorption;Optical films;Optical microscopy;Spatial resolution;Systems biology;Tomography;X-ray detectors;X-ray imaging;LSO:Tb;X-ray detector;luminescence;micro-imaging;micro-tomography;submicrometer resolution;synchrotron instrumentation;thin screen scintillator},
  Timestamp                = {2014.07.01}
}

